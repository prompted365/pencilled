# API Keys
GHL_API_TOKEN=your_gohighlevel_api_token
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CLIENT_SECRET_FILE=your_google_client_secret_file.json

# Business Hours (24-hour format)
BUSINESS_HOURS_START=09:00
BUSINESS_HOURS_END=18:00

# Appointment Settings
APPOINTMENT_BUFFER_MINUTES=15
DEFAULT_APPOINTMENT_DURATION=60
MAX_DAYS_AHEAD=7

# Home Base Location (office/starting point)
HOME_BASE_LAT=40.7128
HOME_BASE_LNG=-74.0060

# GoHighLevel Settings
GHL_CALENDAR_ID=your_ghl_calendar_id
GHL_LOCATION_ID=your_ghl_location_id
GHL_API_VERSION=2021-07-28

# Application Settings
LOG_LEVEL=INFO
CACHE_TTL=3600  # Time-to-live for cached travel times in seconds

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
